@model List<Profile_Mgt.ViewModel.GetImagesViewModel>
@{
    ViewData["Title"] = "GetImages";
    Layout = "~/Views/Shared/_ProfileLayout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-12">
 
            <table class="table table-bordered-none table-responsive" id="tbllist">
                <thead>
                    <tr class="text-center">
                        <th class="text-center">Category Name</th>
                        <th class="text-center">Subcategory Name</th>
                        <th class="text-center">Category Image</th>
                        <th class="text-center">Subcategory Image</th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {
                    <tr class="text-center">
                        <td>@item.CategoryName</td>
                        <td>@item.SubcategoryName</td>
                        <td><img src="@("~/"+item.CategoryImage)" asp-append-version="true" height="100px" width="100px" /></td>
                        <td><img src="@("~/"+item.SubcategoryImage)" asp-append-version="true" height="100px" width="100px" /></td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>


